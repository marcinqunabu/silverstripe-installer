{
	"name": "marcinqunabu/silverstripe-installer",
	"description": "The SilverStripe Framework Installer",
	"require": {
		"php": ">=5.5.0",
		"silverstripe/cms": "3.x-dev",
		"silverstripe/framework": "3.x-dev",
		"silverstripe/reports": "3.x-dev",
		"silverstripe/siteconfig": "3.x-dev",
		"silverstripe-australia/gridfieldextensions": "~1.1",
		"colymba/gridfield-bulk-editing-tools": "^2.1",
		"markguinn/silverstripe-email-helpers": "dev-master",
		"qunabu/silverstripe-theme": "master"
	},
	"require-dev": {
		"phpunit/PHPUnit": "~4.8"
	},
	"extra": {
		"branch-alias": {
			"dev-master": "3.x-dev"
		}
	},
	"config": {
		"process-timeout": 600
	},
	"prefer-stable": true,
	"minimum-stability": "dev"
}